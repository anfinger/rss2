    <titel><![CDATA[Urlaubsreise - Südfrankreich]]></titel>
    <untertitel />
    <zielorte>
    <zielort>
        <name><![CDATA[Roussillon]]></name>
        <land isocode="fr"><![CDATA[Frankreich]]></land>
      </zielort>
      <zielort>
